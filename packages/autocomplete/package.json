{
  "name": "@amzn/autocomplete",
  "private": true,
  "version": "1.0.0",
  "repository": "https://github.com/aws/amazon-q-developer-cli",
  "type": "module",
  "exports": "./dist/index.js",
  "main": "./dist/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc && cp src/index.css dist",
    "build:tailwindcss": "tailwindcss -i src/index.css -o dist/index.css -c tailwind.config.js",
    "dev": "tsc --watch",
    "vite": "vite",
    "lint": "eslint src --report-unused-disable-directives && prettier --check src",
    "lint:fix": "eslint src --fix && prettier --write src",
    "clean": "rm -rf dist/ && rm -rf node_modules/",
    "test": "vitest"
  },
  "browserslist": [
    "defaults",
    "Safari >= 11"
  ],
  "dependencies": {
    "@aws/amazon-q-developer-cli-api-bindings": "workspace:^",
    "@aws/amazon-q-developer-cli-api-bindings-wrappers": "workspace:^",
    "@aws/amazon-q-developer-cli-autocomplete-parser": "workspace:^",
    "@aws/amazon-q-developer-cli-fuzzysort": "workspace:^",
    "@aws/amazon-q-developer-cli-ipc-client-core": "workspace:^",
    "@aws/amazon-q-developer-cli-ipc-client-websocket-mux": "workspace:^",
    "@aws/amazon-q-developer-cli-proto": "workspace:^",
    "@aws/amazon-q-developer-cli-shell-parser": "workspace:^",
    "@bufbuild/protobuf": "2.2.3",
    "@fig/autocomplete-helpers": "^1.0.7",
    "@fig/autocomplete-shared": "^1.1.2",
    "@juggle/resize-observer": "^3.4.0",
    "deep-object-diff": "^1.1.9",
    "emittery": "^1.0.3",
    "exponential-backoff": "^3.1.1",
    "js-yaml": "^4.1.0",
    "loglevel": "^1.9.2",
    "react-dom": "^18.3.1",
    "react-window": "^1.8.11",
    "react": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "use-resize-observer": "^9.1.0",
    "util": "^0.12.5",
    "zustand": "^4.0.0"
  },
  "devDependencies": {
    "@amzn/eslint-config": "workspace:^",
    "@amzn/types": "workspace:^",
    "@aws/amazon-q-developer-cli-fuzzysort": "workspace:^",
    "@aws/amazon-q-developer-cli-shared": "workspace:^",
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.5",
    "@types/react-dom": "^18.3.5",
    "@types/react-window": "^1.8.8",
    "@types/react": "^18.3.18",
    "@types/semver": "^7.5.8",
    "@vitejs/plugin-legacy": "^6.0.0",
    "@vitejs/plugin-react": "^4.3.4",
    "@withfig/autocomplete-types": "^1.31.0",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.16.0",
    "postcss": "^8.4.49",
    "prettier": "^3.4.2",
    "tailwindcss": "^3.4.17",
    "terser": "^5.37.0",
    "typescript": "^5.7.3",
    "vite": "^6.0.0",
    "vitest": "^2.1.8"
  }
}
