{
  "version": "0.5.1",
  "license": "MIT",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "typings": "./dist/cjs/index.d.ts",
  "exports": {
    "require": "./dist/cjs/index.js",
    "import": "./dist/esm/index.js"
  },
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "clean": "rm -rf dist/",
    "build": "yarn clean && yarn build:cjs && yarn build:esm",
    "build:cjs": "tsc --declaration --outDir dist/cjs",
    "build:esm": "tsc --declaration --module esnext --outDir dist/esm",
    "lint": "eslint src",
    "lint:fix": "yarn lint --fix",
    "precommit": "lint-staged",
    "prepublishOnly": "yarn build",
    "generate-requests": "npx ts-node -O '{\"module\":\"commonjs\"}' codegen/generate-requests.ts"
  },
  "name": "@withfig/api-bindings",
  "author": "The Fig Team",
  "devDependencies": {
    "ts-morph": "^15.0.0",
    "ts-node": "^10.3.0",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5"
  },
  "eslintConfig": {
    "extends": "@fig/eslint-config-fig"
  },
  "dependencies": {
    "@fig/eslint-config-fig": "^1.0.0",
    "@tsconfig/recommended": "^1.0.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "eslint": "^8.11.0",
    "lint-staged": "^12.3.5",
    "ts-proto": "^1.82.5"
  },
  "lint-staged": {
    "*.{ts,tsx}": "npx eslint --cache --fix"
  }
}
